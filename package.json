{
  "version": "1.0.3",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "patch": "yarn version --patch && git push origin master && git push origin --tags",
    "minor": "yarn version --minor && git push origin master && git push origin --tags",
    "major": "yarn version --major && git push origin master && git push origin --tags"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.10.9",
    "antd": "^4.10.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-viewer": "^3.2.2"
  },
  "devDependencies": {
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/preset-react": "^1.7.12",
    "@umijs/test": "^3.3.5",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "umi": "3.2.28"
  }
}
